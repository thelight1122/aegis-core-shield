{
    "name": "aegis-core-shield",
    "version": "0.1.0",
    "description": "Non-force governance layer for OpenClaw agents - Discernment Gate + Virtue Integrity System",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "ts-node src/cli/index.ts",
        "gate": "ts-node src/cli/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "build": "tsc",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "aegis",
        "governance",
        "discernment",
        "virtue",
        "integrity",
        "openclaw"
    ],
    "author": "AEGIS Core Team",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.6",
        "jest": "^29.7.0",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {},
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/src"
        ],
        "testMatch": [
            "**/__tests__/**/*.ts",
            "**/?(*.)+(spec|test).ts"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!src/**/*.test.ts",
            "!src/**/*.d.ts"
        ]
    }
}